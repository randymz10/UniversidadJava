<?xml version="1.0" encoding="UTF-8"?>

<configuration status="INFO">
    <Appenders>
        <console name="Console" target="SYSTEM_OUT">
            <PatterLayout pattern="%d{HH:mm::ss} [%t] %-5level %logger{36} -%msg%n"></PatterLayout>
        </console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</configuration>
